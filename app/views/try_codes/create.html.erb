<table class="qr-code">
<% @trycodes.modules.each_index do |x| %>
  <tr>
  <% @trycodes.modules.each_index do |y| %>
   <% if @trycodes.dark?(x,y) %>
    <td class="black"/>
   <% else %>
    <td class="white"/>
   <% end %>
  <% end %>
  </tr>
<% end %>
</table>
<br />
<br />
<%= link_to "New QR Code", new_try_code_path %>