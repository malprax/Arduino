$("ul.errors").html("")
<% if @member.errors.any? %>
  <% @member.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".member-index").html("<%= escape_javascript(render 'index') %>")
  $("#member-modal").modal("hide")
<% end %>