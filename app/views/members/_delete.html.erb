<div class="modal-dialog">
  <div class="modal-content">
  <div class="modal-header">
    <h3>Hapus Member</h3>
  </div>
  <div class="modal-body">
  <ul class="errors"></ul>
  Apakah Anda Ingin Menghapus Member <b><%= @member.name %></b>?
  </div>
  <div class="modal-footer">
    <%= link_to "Ya, Hapus Member Ini", member_path(@member), method: :delete, remote: true, class: "btn btn-danger" %>
    <%= link_to "Tidak", "#", class: "btn btn-default", data: { dismiss: "modal" } %>
  </div>
  </div>
</div>