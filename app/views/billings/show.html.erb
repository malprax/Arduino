
<%= render "parkir" %>

<% url = "#{@billing.id}.pdf" %> 
<iframe id ="pdf_source" src="<%= url %>" style="width:700px;height:500px;"></iframe>

<script>
$(document).ready(function(){
  pdf = document.getElementById('pdf_source');
  pdf.focus();

  setTimeout(function(){
      pdf.contentWindow.print();
  }, 3000);
})
  
</script>
